{
	"name": "module-name-helper",
	"displayName": "module-name-helper",
	"description": "",
	"version": "0.0.1",
	"publisher": "marcinkowal2015",
	"author": {
		"name": "Marcin Kowal",
		"email": "marcinkowal2015@outlook.com",
		"url": "https://marcinkowal.dev"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.showModuleName"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.showModuleName",
				"title": "Show module name"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.showModuleName",
					"group": "showModuleNameGroup",
					"when": "editorTextFocus "
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"module-name-resolver": "0.0.3"
	}
}
